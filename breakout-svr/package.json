{
  "name": "breakout-svr",
  "version": "0.4.1",
  "private": true,
  "scripts": {
    "start": "pm2 start process.json",
    "production": "pm2 start process.json --env production",
    "restart": "pm2 restart process.json",
    "stop": "pm2 stop process.json",
    "build": "npm run clean-app && tsc --outDir ./dist",
    "watch": "npm run clean-app && tsc --watch --outDir ./dist",
    "test": "NODE_ENV=test mocha --require espower-typescript/guess \"./src/test/**/*.ts\"",
    "doc": "npm run clean-doc && typedoc ./src --exclude /**/test/**/*.ts --out ./out --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -rf ./dist",
    "clean-doc": "rm -rf ./out",
    "tslint": "tslint \"./src/**/*.ts\""
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/body-parser": "^1.16.4",
    "@types/config": "^0.0.34",
    "@types/connect-redis": "^0.0.7",
    "@types/cookie-parser": "^1.3.30",
    "@types/ejs": "^2.3.33",
    "@types/express": "^4.0.34",
    "@types/express-session": "^1.15.1",
    "@types/log4js": "^0.0.32",
    "@types/node": "^10.1.2",
    "@types/passport": "^0.4.5",
    "@types/passport-local": "^1.0.29",
    "@types/random-js": "^1.0.30",
    "@types/redis": "^2.6.0",
    "@types/sequelize": "^4.0.66",
    "@types/source-map-support": "^0.4.0",
    "@types/string": "^0.0.29",
    "@types/swagger-jsdoc": "^0.0.2",
    "accept-language-parser": "^1.2.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "connect-redis": "^3.3.0",
    "cookie-parser": "~1.4.3",
    "crypt": "0.0.2",
    "debug": "~3.1.0",
    "ejs": "^2.5.5",
    "express": "^4.15.3",
    "express-promise-router": "^3.0.2",
    "express-session": "^1.15.4",
    "js-yaml": "^3.6.1",
    "log4js": "^1.1.0",
    "mysql2": "^1.3.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "random-js": "^1.0.8",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.4.1",
    "sequelize-typescript": "^0.6.4",
    "source-map-support": "^0.5.6",
    "string": "^3.3.3"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^8.1.3",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.5.5",
    "node-pre-gyp": "^0.10.0",
    "pm2": "^2.10.4",
    "power-assert": "^1.5.0",
    "swagger-jsdoc": "^1.9.4",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3"
  }
}
