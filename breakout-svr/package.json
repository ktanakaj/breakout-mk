{
  "name": "breakout-svr",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "start": "pm2 start process.json",
    "production": "pm2 start process.json --env production",
    "restart": "pm2 restart process.json",
    "stop": "pm2 stop process.json",
    "build": "npm run clean-app && tsc --outDir ./lib",
    "watch": "npm run clean-app && tsc --watch --outDir ./lib",
    "test": "NODE_ENV=test mocha --compilers ts:espower-typescript/guess --recursive ./src/test",
    "doc": "npm run clean-doc && typedoc ./src --exclude /**/test/**/*.ts --out ./out --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -rf ./lib",
    "clean-doc": "rm -rf ./out",
    "tslint": "tslint ./src"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/body-parser": "^0.0.33",
    "@types/config": "^0.0.31",
    "@types/connect-redis": "0.0.5",
    "@types/cookie-parser": "^1.3.30",
    "@types/ejs": "^2.3.33",
    "@types/express": "^4.0.34",
    "@types/express-session": "0.0.32",
    "@types/log4js": "^0.0.32",
    "@types/node": "^8.0.13",
    "@types/passport": "^0.3.1",
    "@types/passport-local": "^1.0.29",
    "@types/random-js": "1.0.28",
    "@types/redis": "^2.6.0",
    "@types/sequelize": "^4.0.39",
    "@types/source-map-support": "^0.4.0",
    "@types/string": "0.0.28",
    "@types/swagger-jsdoc": "^0.0.1",
    "accept-language-parser": "^1.2.0",
    "bluebird": "^3.5.0",
    "body-parser": "~1.15.1",
    "config": "^1.21.0",
    "connect-redis": "^3.3.0",
    "cookie-parser": "~1.4.3",
    "crypt": "0.0.2",
    "debug": "~2.6.0",
    "ejs": "^2.5.5",
    "express": "^4.15.3",
    "express-promise-router": "^2.0.0",
    "express-session": "^1.14.2",
