{
  "name": "breakout-svr",
  "version": "0.4.1",
  "private": true,
  "scripts": {
    "start": "pm2 start process.json",
    "production": "pm2 start process.json --env production",
    "restart": "pm2 restart process.json",
    "stop": "pm2 stop process.json",
    "build": "npm run clean-app && tsc --outDir ./dist",
    "watch": "npm run clean-app && tsc --watch --outDir ./dist",
    "test": "NODE_ENV=test mocha --require espower-typescript/guess \"./test/**/*.ts\" --exit",
    "doc": "npm run clean-doc && typedoc ./src --out ./doc --mode file --ignoreCompilerErrors",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -rf ./dist",
    "clean-doc": "rm -rf ./doc",
    "lint": "tslint \"./src/**/*.ts\" \"./test/**/*.ts\""
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.11.4"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/config": "^0.0.34",
    "@types/connect-redis": "^0.0.7",
    "@types/cookie-parser": "^1.3.30",
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/node": "^10.5.7",
    "@types/passport": "^0.4.6",
    "@types/passport-local": "^1.0.29",
    "@types/random-js": "^1.0.30",
    "@types/redis": "^2.6.0",
    "@types/sequelize": "^4.27.24",
    "@types/source-map-support": "^0.4.1",
    "@types/string": "^0.0.29",
    "@types/swagger-express-validator": "0.0.0",
    "@types/swagger-jsdoc": "0.0.2",
    "accept-language-parser": "^1.2.0",
    "body-parser": "^1.18.3",
    "config": "^2.0.1",
    "connect-redis": "^3.3.0",
    "cookie-parser": "~1.4.3",
    "crypt": "0.0.2",
    "debug": "~3.1.0",
    "ejs": "^2.5.5",
    "express": "^4.15.3",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.15.4",
    "js-yaml": "^3.12.0",
    "log4js": "^3.0.4",
    "mysql2": "^1.6.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "random-js": "^1.0.8",
    "redis": "^2.7.1",
    "redis-promisify": "^0.1.1",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.38.0",
    "sequelize-typescript": "^0.6.6",
    "source-map-support": "^0.5.6",
    "string": "^3.3.3",
    "swagger-express-validator": "^0.1.0",
    "swagger-jsdoc": "^3.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.0",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.5.5",
    "node-pre-gyp": "^0.10.3",
    "pm2": "^3.0.3",
    "power-assert": "^1.6.0",
    "sqlite3": "^4.0.2",
    "tslint": "^5.11.0",
    "typedoc": "^0.11.1",
    "typescript": "^3.0.1"
  }
}
