{
  "name": "breakout-web",
  "version": "0.4.2",
  "private": true,
  "scripts": {
    "build": "npm run clean-app && webpack",
    "watch": "npm run clean-app && webpack --watch --devtool source-map",
    "test": "NODE_ENV=test karma start",
    "doc": "npm run clean-doc && typedoc ./public/app --exclude /**/test/**/*.ts --ignoreCompilerErrors --out ./doc --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -f ./public/bundle.js ./public/bundle.js.map",
    "clean-doc": "rm -rf ./doc",
    "lint": "tslint \"./public/app/**/*.ts\" \"./test/**/*.ts\""
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^6.1.3",
    "@angular/compiler": "^6.1.3",
    "@angular/core": "^6.1.3",
    "@angular/forms": "^6.1.3",
    "@angular/platform-browser": "^6.1.3",
    "@angular/platform-browser-dynamic": "^6.1.3",
    "@angular/router": "^6.1.3",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/node": "^10.7.1",
    "ngx-bootstrap": "^3.0.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "jasmine-core": "^3.2.1",
    "karma": "^2.0.5",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.12.0",
    "typescript": "^2.9.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^2.1.5"
  }
}
