{
  "name": "breakout-web",
  "version": "0.4.1",
  "private": true,
  "scripts": {
    "build": "npm run clean-app && webpack",
    "watch": "npm run clean-app && webpack --watch --devtool source-map",
    "test": "NODE_ENV=test karma start",
    "doc": "npm run clean-doc && typedoc ./public/app --exclude /**/test/**/*.ts --ignoreCompilerErrors --out ./out --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -f ./public/bundle.js ./public/bundle.js.map",
    "clean-doc": "rm -rf ./out",
    "tslint": "tslint \"./public/app/**/*.ts\" \"./test/**/*.ts\""
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/node": "^10.1.2",
    "ngx-bootstrap": "^3.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.54",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.2",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "ts-loader": "^4.1.0",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  }
}
