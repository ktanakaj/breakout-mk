{
  "name": "breakout-web",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "build": "npm run clean-app && webpack",
    "watch": "npm run clean-app && webpack --watch --devtool source-map",
    "test": "NODE_ENV=test karma start",
    "doc": "npm run clean-doc && typedoc ./public/app --exclude /**/test/**/*.ts --ignoreCompilerErrors --out ./out --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -f ./public/bundle.js ./public/bundle.js.map",
    "clean-doc": "rm -rf ./out",
    "tslint": "tslint ./public/app ./test"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "@ngx-translate/core": "^7.1.0",
    "@ngx-translate/http-loader": "^0.1.0",
    "ngx-bootstrap": "^1.8.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "core-js": "^2.5.0",
    "jasmine-core": "^2.7.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "ts-loader": "^2.1.0",
    "tslint": "^5.3.2",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.3",
    "webpack": "^3.3.0"
  }
}
